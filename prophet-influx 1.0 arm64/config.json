{
    "name": "Prophet Influx API (ARM64)",
    "version": "1.0",
    "slug": "prophet-influx",
    "description": "An API add-on for forecasting with Prophet and Influx (ARM64 version)",
    "arch": ["armhf", "armv7", "aarch64", "amd64"],
    "startup": "services",
    "boot": "auto",
    "options": {
        "influxdb_host": "localhost",
        "influxdb_port": 8086,
        "influxdb_user": "homeassistant",
        "influxdb_password": "",
        "influxdb_dbname": "homeassistant",
        "port": 5000
    },
    "schema": {
        "influxdb_host": "str",
        "influxdb_port": "int",
        "influxdb_user": "str",
        "influxdb_password": "str",
        "influxdb_dbname": "str",
        "port": "int"
    },
    "image": "mgenrique/prophet-influx:1.0_arm64",
    "ports": {
        "5000/tcp": 5000
    },
    "host_network": false,
    "environment": {
      "INFLUXDB_HOST": "options.influxdb_host",
      "INFLUXDB_PORT": "options.influxdb_port",
      "INFLUXDB_USER": "options.influxdb_user",
      "INFLUXDB_PASSWORD": "options.influxdb_password",
      "INFLUXDB_DBNAME": "options.influxdb_dbname",
      "PORT": "options.port"
  }    
}
